<script>
function sendchat(message)
{
		
	$.ajax ({
	type: "POST",
	url:"./chat.php",
	data: {message:message},
	success: function(data) {
		document.getElementById("chatlist").innerHTML = data;
	}
	});
}
function refresh()
{
		$.ajax ({
		type: "POST",
		url:"./chat.php",
		data: {refresh:"test"},
		success: function(data) {
			document.getElementById("chatlist").innerHTML = data;
	   }
	});
}
</script>

